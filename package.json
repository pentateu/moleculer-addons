{
  "name": "moleculer-db-adapter-mongoose",
  "version": "0.3.1",
  "description": "Mongoose adapter for Moleculer DB service",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon examples/index.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "microservice",
    "moleculer"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ice-services/moleculer-addons.git"
  },
  "homepage": "https://github.com/ice-services/moleculer-addons/tree/master/packages/moleculer-db-adapter-mongoose#readme",
  "author": "Ice Services",
  "license": "MIT",
  "peerDependencies": {
    "moleculer": "^0.8.0"
  },
  "devDependencies": {
    "benchmarkify": "2.1.0",
    "coveralls": "2.13.1",
    "eslint": "4.2.0",
    "fakerator": "0.3.0",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "lolex": "2.0.0",
    "moleculer": "0.8.2",
    "nodemon": "1.11.0",
    "npm-check": "5.4.5"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/"
    ]
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "lodash": "4.17.4",
    "mongoose": "4.11.3"
